<%- include('./partials/header')  %>
<div class="ui main text container segment">
    <div class="ui huge header">Contact for help</div>
	<form class="ui form" action="/help" method="POST">
        <div class="field">
            <label>Name</label>
            <input type="text" name="contact[name]" placeholder="name">
        </div>
        <div class="field">
            <label>email</label>
            <input type="email" name="contact[email]" placeholder="something@xyz.com ">
			
        </div>
        <div class="field">
            <label>Message</label>
            <textarea name="contact[message]"></textarea>
        </div>
        <input class="ui violet big basic button" type="submit">
    </form>
   
</div>
